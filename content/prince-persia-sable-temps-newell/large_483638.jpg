}
function cgRefresh(formatId, formatWidth, formatHeight)
{
	siteId = 37540;	
	pageId = 256129;
	if (targetDiv = document.getElementById('sas_'+formatId)) //pas ==
	{
		targetDiv.parentNode.innerHTML = '<div id="sas_'+formatId+'"></div><iframe width="'+formatWidth+'" height="'+formatHeight+'" id="sas_i'+formatId+'" onload="cgresize(this)" frameborder="0" scrolling="no" style="margin: 0px; padding: 0px; width: '+formatWidth+'px; height: '+formatHeight+'px;">';
		iframe = document.getElementById('sas_i'+formatId);
		
		url = 'http://www.smartadserver.com/ac?out=js&nwid=104&siteid='+siteId+'&pgid='+pageId+'&fmtid='+formatId+'&visit=m&tmstp=[timestamp]';
		val = '<html><head></head><body style="margin:0;padding:0">';
		val += '<scr' + 'ipt type="text/javascript" src="' + url + '"></scr' + 'ipt>';
		val += '</body></html>';
		
		var doc = iframe.contentWindow || iframe.contentDocument;
		if (doc.document) doc = doc.document
		doc.open();
		doc.write(val);
		doc.close();
	}
}

